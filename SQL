SELECT DISTINCT 
AccountID, Number 		
OYBAccountID,	Number Nullable
JobID, Number
ListID,	Number
BatchID,	Number
SubscriberID,	Number
SubscriberKey, Number Primary Key		
EventDate, Date
IsUnique, Boolean
Domain,	text
BounceCategoryID, Number
BounceCategory, text Nullable
BounceSubcategoryID, Number Nullable
BounceSubcategory, Text Nullable
BounceTypeID,	Number
BounceType, Number Nullable
SMTPBounceReason, text Nullable
SMTPMessage, text Nullable
TriggererSendDefinitionObjectID,text Nullable
TriggeredSendCustomerKey text Nullable
FROM _Bounce 
where 
BounceCategory = 'Hard Bounce' 
and IsUnique = 1



